{% extends "::base.html.twig" %}

{% block body %}
    <h1>Email creation</h1>

      <form action="{{ path('email_create') }}" method="post" {{ form_enctype(form) }}>
          {{ form_widget(form) }}
          <div class="actions">
              <button type="submit" class="btn primary">Create</button>
              <a href="{{ path('email') }}" class="btn secondary">
                  Back to the list
              </a>
          </div>
      </form>
{% endblock %}